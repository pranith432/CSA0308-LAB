#include <stdio.h>
#define INF 9999
int minDistance(int dist[], int visited[], int n){
    int min=INF, min_index;
    for(int v=0;v<n;v++)
        if(!visited[v] && dist[v]<=min){ min=dist[v]; min_index=v; }
    return min_index;
}

int main(){
    int n; printf("Enter number of vertices: "); scanf("%d",&n);
    int graph[10][10];
    printf("Enter adjacency matrix:\n");
    for(int i=0;i<n;i++) for(int j=0;j<n;j++) scanf("%d",&graph[i][j]);
    int dist[10], visited[10]={0};
    for(int i=0;i<n;i++) dist[i]=INF;
    dist[0]=0;

    for(int count=0;count<n-1;count++){
        int u=minDistance(dist,visited,n);
        visited[u]=1;
        for(int v=0;v<n;v++)
            if(!visited[v] && graph[u][v] && dist[u]+graph[u][v]<dist[v])
                dist[v]=dist[u]+graph[u][v];
    }

    printf("Vertex\tDistance from Source\n");
    for(int i=0;i<n;i++) printf("%d\t%d\n",i,dist[i]);
    return 0;
}
