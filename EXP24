#include <stdio.h>
#define INF 9999
int main(){
    int n; printf("Enter number of vertices: "); scanf("%d",&n);
    int cost[10][10], selected[10]={0};
    printf("Enter adjacency matrix:\n");
    for(int i=0;i<n;i++) for(int j=0;j<n;j++) scanf("%d",&cost[i][j]);
    selected[0]=1; int edge_count=0, min, x=0, y=0;
    printf("Edges in MST:\n");
    while(edge_count<n-1){
        min=INF;
        for(int i=0;i<n;i++){
            if(selected[i]){
                for(int j=0;j<n;j++){
                    if(!selected[j] && cost[i][j]){
                        if(cost[i][j]<min){ min=cost[i][j]; x=i; y=j; }
                    }
                }
            }
        }
        printf("%d - %d : %d\n",x,y,cost[x][y]);
        selected[y]=1; edge_count++;
    }
    return 0;
}
