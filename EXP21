#include <stdio.h>
#define MAX 50
int queue[MAX], front=-1, rear=-1;
void enqueue(int v){ queue[++rear]=v; if(front==-1) front=0; }
int dequeue(){ return queue[front++]; }
int isEmpty(){ return front>rear || front==-1; }

int main(){
    int n,i,j,adj[10][10],visited[10]={0};
    printf("Enter number of vertices: "); scanf("%d",&n);
    printf("Enter adjacency matrix:\n");
    for(i=0;i<n;i++) for(j=0;j<n;j++) scanf("%d",&adj[i][j]);

    int start=0;
    enqueue(start); visited[start]=1;
    printf("BFS: ");
    while(!isEmpty()){
        int v=dequeue(); printf("%d ",v);
        for(i=0;i<n;i++){
            if(adj[v][i] && !visited[i]){
                enqueue(i); visited[i]=1;
            }
        }
    }
    return 0;
}
