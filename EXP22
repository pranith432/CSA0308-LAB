#include <stdio.h>
int visited[10], n, adj[10][10];

void dfs(int v){
    visited[v]=1;
    printf("%d ",v);
    for(int i=0;i<n;i++)
        if(adj[v][i] && !visited[i]) dfs(i);
}

int main(){
    int i,j;
    printf("Enter number of vertices: "); scanf("%d",&n);
    printf("Enter adjacency matrix:\n");
    for(i=0;i<n;i++) for(j=0;j<n;j++) scanf("%d",&adj[i][j]);
    for(i=0;i<n;i++) visited[i]=0;

    printf("DFS: "); dfs(0);
    return 0;
}
